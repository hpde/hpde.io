      </TemporalDescription>
      <!--InstrumentRegion=Heliosphere.NearEarth-->
      <ObservedRegion>Heliosphere.NearEarth</ObservedRegion>
      <Parameter>
         <Name>Magnetic field vector, GSE</Name>
         <ParameterKey>Magnetic field vector, GSE</ParameterKey>
         <Description>Magnetic field vector, GSE</Description>
         <Cadence>PT5M</Cadence>
         <Units>nT</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
            <CoordinateSystemName>GSE</CoordinateSystemName>
         </CoordinateSystem>
         <Field>
            <Qualifier>Component</Qualifier>
            <Qualifier>Average</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Magnetic field vector, GSM</Name>
         <ParameterKey>Magnetic field vector, GSM</ParameterKey>
         <Description>Magnetic field vector, GSM</Description>
         <Cadence>PT5M</Cadence>
         <Units>nT</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
            <CoordinateSystemName>GSM</CoordinateSystemName>
         </CoordinateSystem>
         <Field>
            <Qualifier>Component</Qualifier>
            <Qualifier>Average</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Magnetic field magnitude</Name>
         <ParameterKey>Magnetic field magnitude</ParameterKey>
         <Description>Magnetic field magnitude</Description>
         <Cadence>PT5M</Cadence>
         <Units>nT</Units>
         <Field>
            <Qualifier>Magnitude</Qualifier>
            <Qualifier>Average</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Magnetic field vector standard deviation</Name>
         <ParameterKey>Magnetic field vector standard deviation</ParameterKey>
         <Description>Square root of sum of variances of component averages.</Description>
         <Cadence>PT5M</Cadence>
         <Units>nT</Units>
         <Field>
            <Qualifier>Variance</Qualifier>
            <Qualifier>Component</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Magnetic field magnitude standard deviation</Name>
         <ParameterKey>Magnetic field magnitude standard deviation</ParameterKey>
         <Description>Standard deviation in the average of magnetic field magnitudes</Description>
         <Cadence>PT5M</Cadence>
         <Units>nT</Units>
         <Field>
            <Qualifier>Variance</Qualifier>
            <Qualifier>Magnitude</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Electric field</Name>
         <ParameterKey>Electric field</ParameterKey>
         <Description>Electric field, -  V  * Bz(GSM) * 10**-3</Description>
         <Cadence>PT5M</Cadence>
         <Units>mV/m</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
            <CoordinateSystemName>GSM</CoordinateSystemName>
         </CoordinateSystem>
         <Field>
            <Qualifier>Component</Qualifier>
            <FieldQuantity>Electric</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Flow speed</Name>
         <ParameterKey>Flow speed</ParameterKey>
         <Description>Flow speed of the solar wind</Description>
         <Cadence>PT5M</Cadence>
         <Units>km/s</Units>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <Qualifier>Magnitude</Qualifier>
            <ParticleQuantity>Velocity</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Flow velocity vector</Name>
         <ParameterKey>Flow velocity vector</ParameterKey>
         <Description>Flow velocity vector, 3 GSE cartesian components</Description>
         <Cadence>PT5M</Cadence>
         <Units>km/s</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
            <CoordinateSystemName>GSE</CoordinateSystemName>
         </CoordinateSystem>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <Qualifier>Vector</Qualifier>
            <ParticleQuantity>Velocity</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Proton density</Name>
         <ParameterKey>Proton density</ParameterKey>
         <Description>Proton density</Description>
         <Cadence>PT5M</Cadence>
         <Units>no./cc</Units>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <ParticleQuantity>NumberDensity</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Proton temperature</Name>
         <ParameterKey>Proton temperature</ParameterKey>
         <Description>Proton scalar temperature</Description>
         <Cadence>PT5M</Cadence>
         <Units>deg K</Units>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <ParticleQuantity>Temperature</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Flow pressure</Name>
         <ParameterKey>Flow pressure</ParameterKey>
         <Description>Flow pressure, NmV**2</Description>
         <Cadence>PT5M</Cadence>
         <Units>nPa</Units>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <ParticleQuantity>Pressure</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Plasma beta</Name>
         <ParameterKey>Plasma beta</ParameterKey>
         <Description>Plasma beta = [(T*4.16/10**5) + 5.34] * Np / B**2 (B in nT)</Description>
         <Cadence>PT5M</Cadence>
         <Mixed>
            <MixedQuantity>PlasmaBeta</MixedQuantity>
         </Mixed>
      </Parameter>
      <Parameter>
         <Name>Alfven Mach Number</Name>
         <ParameterKey>Alfven Mach Number</ParameterKey>
         <Description>Alfven Mach number = (V * Np**0.5) / 20 * B</Description>
         <Cadence>PT5M</Cadence>
         <Mixed>
            <MixedQuantity>AlfvenMachNumber</MixedQuantity>
         </Mixed>
      </Parameter>
      <Parameter>
         <Name>Spacecraft position</Name>
         <ParameterKey>Spacecraft position</ParameterKey>
         <Description>Position of whichever spacecraft&apos;s (ACE, Wind, etc.) data were used for this 5-min interval, GSE cartesian components</Description>
         <Cadence>PT5M</Cadence>
         <Units>Re</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
            <CoordinateSystemName>GSE</CoordinateSystemName>
         </CoordinateSystem>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Bow shock nose position</Name>
         <ParameterKey>Bow shock nose position</ParameterKey>
         <Description>Bow shock nose position, as computed from observed data and models, 3 cartesian components</Description>
         <Cadence>PT5M</Cadence>
         <Units>Re</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
            <CoordinateSystemName>GSE</CoordinateSystemName>
         </CoordinateSystem>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Mean time shift</Name>
         <ParameterKey>Mean time shift</ParameterKey>
         <Description>5-min average over 1-min time shifts from observation time to bow shock nose (BSN) arrival time, as averaged over all 15-16 sec B values with BSN arrival times in each minute of interest</Description>
         <Cadence>PT5M</Cadence>
         <Units>sec</Units>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Spacecraft IDs</Name>
         <ParameterKey>Spacecraft IDs</ParameterKey>
         <Description>Identifier(s) of the spacecraft whose (a) field and (b) plasma data were used for this 5-min interval</Description>
         <Cadence>PT5M</Cadence>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Numbers of points in averages</Name>
         <ParameterKey>Numbers of points in averages</ParameterKey>
         <Description>Numbers of 1-min points in 5-min field and plasma averages</Description>
         <Cadence>PT5M</Cadence>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
   </NumericalData>
