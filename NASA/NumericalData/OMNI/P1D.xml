         <Field>
            <Qualifier>Component</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Standard deviations in GSE component averages</Name>
         <Description>Standard deviations in averaged GSE Cartesian components of magnetic field vector</Description>
         <Cadence>P1D</Cadence>
