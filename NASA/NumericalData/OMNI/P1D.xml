               <Qualifier>Component.I</Qualifier>
               <Index>1</Index>
               <ParameterKey>Column 13</ParameterKey>
            </Element>
            <Element>
               <Name>By, GSM</Name>
               <Qualifier>Component.J</Qualifier>
               <Index>2</Index>
               <ParameterKey>Column 16</ParameterKey>
            </Element>
            <Element>
               <Name>Bz, GSM</Name>
