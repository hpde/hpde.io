         <Structure>
            <Size>5</Size>
            <Element>
               <Name>Year</Name>
               <Index>1</Index>
               <ValidMin>1991</ValidMin>
               <ValidMax>2008</ValidMax>
               <FillValue>-9999</FillValue>
            </Element>
            <Element>
               <Name>Day of year</Name>
               <Index>2</Index>
               <ValidMin>1</ValidMin>
               <ValidMax>366</ValidMax>
               <FillValue>-999</FillValue>
