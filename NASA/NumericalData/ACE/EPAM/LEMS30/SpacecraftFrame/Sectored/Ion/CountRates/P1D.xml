            <Element>
               <Name>Minute of hour</Name>
               <Index>4</Index>
               <ValidMin>0</ValidMin>
               <ValidMax>59</ValidMax>
               <FillValue>-99</FillValue>
            </Element>
            <Element>
               <Name>Seconds of minute</Name>
               <Index>5</Index>
               <ValidMin>0.00</ValidMin>
               <ValidMax>59.99</ValidMax>
               <FillValue>-99.99</FillValue>
