    <InstrumentID>spase://SMWG/Instrument/Helios1/E1</InstrumentID>
    <InstrumentID>spase://SMWG/Instrument/Helios1/E3</InstrumentID>
    <MeasurementType>MagneticField</MeasurementType>
    <MeasurementType>ThermalPlasma</MeasurementType>
    <TemporalDescription>
      <TimeSpan>
        <StartDate>1974-12-12T00:00:00</StartDate>
        <StopDate>1981-06-14T00:00:00</StopDate>
        <Note>Plasma data end on day 364 of 1980.</Note>
      </TimeSpan>
      <Cadence>PT1H</Cadence>
    </TemporalDescription>
    <ObservedRegion>Heliosphere.Inner</ObservedRegion>
    <Parameter>
      <Name>Year</Name>
      <ParameterKey>Column1</ParameterKey>
      <Description>Four digit Year</Description>
      <Cadence>PT1H</Cadence>
      <FillValue>999.9999</FillValue>
      <Support>
        <SupportQuantity>Temporal</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Day</Name>
      <ParameterKey>Column2</ParameterKey>
      <Description>Decimal Day of Year</Description>
      <Cadence>PT1H</Cadence>
      <FillValue>999.9999</FillValue>
      <Support>
        <SupportQuantity>Temporal</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Hour</Name>
      <ParameterKey>Column3</ParameterKey>
      <Description>Hour</Description>
      <Cadence>PT1H</Cadence>
      <FillValue>999.9999</FillValue>
      <Support>
        <SupportQuantity>Temporal</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Carrington Rotation Number</Name>
      <ParameterKey>Column4</ParameterKey>
      <Description>Carrington Rotation Number seen by an Earth based observer at the start of the data interval</Description>
      <Cadence>PT1H</Cadence>
      <FillValue>999.9999</FillValue>
      <Support>
        <SupportQuantity>Temporal</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>H1 radial distance</Name>
      <ParameterKey>Column05</ParameterKey>
      <Description>Heliocentric radial distance of Helios 1 in AU</Description>
      <Cadence>PT1H</Cadence>
      <Units>AU</Units>
      <Support>
        <SupportQuantity>Positional</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>H1 latitude</Name>
      <ParameterKey>Column06</ParameterKey>
      <Description>Heliographic Inertial elevation angle of Helios 1 in deg</Description>
      <Cadence>PT1H</Cadence>
      <Units>deg</Units>
      <CoordinateSystem>
        <CoordinateRepresentation>Spherical</CoordinateRepresentation>
        <CoordinateSystemName>HGI</CoordinateSystemName>
      </CoordinateSystem>
      <Support>
        <SupportQuantity>Positional</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>H1 longitude</Name>
      <ParameterKey>Column07</ParameterKey>
      <Description>Heliographic Inertial azimuthal angle of Helios 1 in deg</Description>
      <Cadence>PT1H</Cadence>
      <Units>deg</Units>
      <CoordinateSystem>
        <CoordinateRepresentation>Spherical</CoordinateRepresentation>
        <CoordinateSystemName>HGI</CoordinateSystemName>
      </CoordinateSystem>
      <Support>
        <SupportQuantity>Positional</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>ESS angle</Name>
      <ParameterKey>Column08</ParameterKey>
      <Description>Earth-Sun-Spacecraft separation angle</Description>
      <Cadence>PT1H</Cadence>
      <Units>deg</Units>
      <Support>
        <SupportQuantity>Positional</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Magnetic Field Vector, SSE</Name>
      <Description>Hourly averages of SSE (spacecraft-centric solar ecliptic) components of fine time scale magnetic field vectors</Description>
      <Cadence>PT1H</Cadence>
      <Units>nT</Units>
      <CoordinateSystem>
        <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
        <CoordinateSystemName>SSE</CoordinateSystemName>
      </CoordinateSystem>
      <Structure>
        <Size>3</Size>
        <Description>X,Y,Z components</Description>
        <Element>
          <Name>X component</Name>
          <Qualifier>Component.I</Qualifier>
          <Index>1</Index>
          <ParameterKey>Column09</ParameterKey>
        </Element>
        <Element>
          <Name>Y component</Name>
          <Qualifier>Component.J</Qualifier>
          <Index>2</Index>
          <ParameterKey>Column10</ParameterKey>
        </Element>
        <Element>
          <Name>Z component</Name>
          <Qualifier>Component.K</Qualifier>
          <Index>3</Index>
          <ParameterKey>Column11</ParameterKey>
        </Element>
      </Structure>
      <FillValue>99999.99</FillValue>
      <Field>
        <Qualifier>Vector</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Magnetic Field Vector</Name>
      <Description>Hourly averages of RTN components of fine time scale magnetic field vectors</Description>
      <Cadence>PT1H</Cadence>
      <Units>nT</Units>
      <CoordinateSystem>
        <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
        <CoordinateSystemName>RTN</CoordinateSystemName>
      </CoordinateSystem>
      <Structure>
        <Size>3</Size>
        <Description>R,T,N components</Description>
        <Element>
          <Name>R component</Name>
          <Qualifier>Component.I</Qualifier>
          <Index>1</Index>
          <ParameterKey>Column12</ParameterKey>
        </Element>
        <Element>
          <Name>T component</Name>
          <Qualifier>Component.J</Qualifier>
          <Index>2</Index>
          <ParameterKey>Column13</ParameterKey>
        </Element>
        <Element>
          <Name>N component</Name>
          <Qualifier>Component.K</Qualifier>
          <Index>3</Index>
          <ParameterKey>Column14</ParameterKey>
        </Element>
      </Structure>
      <FillValue>99999.99</FillValue>
      <Field>
        <Qualifier>Vector</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Magnetic Field Magnitude</Name>
      <ParameterKey>Column15</ParameterKey>
      <Description>Hourly average of fine time scale magnetic field magnitudes</Description>
      <Cadence>PT1H</Cadence>
      <Units>nT</Units>
      <FillValue>99999.99</FillValue>
      <Field>
        <Qualifier>Magnitude</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Flow speed</Name>
      <ParameterKey>Column16</ParameterKey>
      <Description>Hourly averaged solar wind plasma flow speed</Description>
      <Cadence>PT1H</Cadence>
      <Units>km/s</Units>
      <FillValue>9999.9</FillValue>
      <Particle>
        <ParticleType>Proton</ParticleType>
        <Qualifier>Average</Qualifier>
        <ParticleQuantity>FlowSpeed</ParticleQuantity>
      </Particle>
    </Parameter>
    <Parameter>
      <Name>Flow elevation angle</Name>
      <ParameterKey>Column17</ParameterKey>
      <Description>Hourly averaged solar wind plasma flow elevation angle</Description>
      <Cadence>PT1H</Cadence>
      <Units>deg</Units>
      <CoordinateSystem>
        <CoordinateRepresentation>Spherical</CoordinateRepresentation>
        <CoordinateSystemName>RTN</CoordinateSystemName>
      </CoordinateSystem>
      <FillValue>9999.9</FillValue>
      <Particle>
        <ParticleType>Proton</ParticleType>
        <Qualifier>DirectionAngle.ElevationAngle</Qualifier>
        <ParticleQuantity>FlowVelocity</ParticleQuantity>
      </Particle>
    </Parameter>
    <Parameter>
      <Name>Flow azimuth angle</Name>
      <ParameterKey>Column18</ParameterKey>
      <Description>Hourly averaged solar wind plasma flow azimuthal angle</Description>
      <Cadence>PT1H</Cadence>
      <CoordinateSystem>
        <CoordinateRepresentation>Spherical</CoordinateRepresentation>
        <CoordinateSystemName>RTN</CoordinateSystemName>
      </CoordinateSystem>
      <FillValue>9999.9</FillValue>
      <Particle>
        <ParticleType>Proton</ParticleType>
        <Qualifier>DirectionAngle.AzimuthAngle</Qualifier>
        <ParticleQuantity>FlowVelocity</ParticleQuantity>
      </Particle>
    </Parameter>
    <Parameter>
      <Name>Density</Name>
      <ParameterKey>Column19</ParameterKey>
      <Description>Hourly averaged solar wind plasma density</Description>
      <Cadence>PT1H</Cadence>
      <Units>n/cc</Units>
      <FillValue>999.9</FillValue>
      <Particle>
        <ParticleType>Proton</ParticleType>
        <Qualifier>Average</Qualifier>
        <ParticleQuantity>NumberDensity</ParticleQuantity>
      </Particle>
    </Parameter>
    <Parameter>
      <Name>Temperature</Name>
      <ParameterKey>Column20</ParameterKey>
      <Description>Hourly averaged solar wind plasma temperature (radial component)</Description>
      <Cadence>PT1H</Cadence>
      <Units>degK</Units>
      <FillValue>9999999.</FillValue>
      <Particle>
        <ParticleType>Proton</ParticleType>
        <Qualifier>Average</Qualifier>
        <ParticleQuantity>Temperature</ParticleQuantity>
      </Particle>
    </Parameter>
  </NumericalData>
