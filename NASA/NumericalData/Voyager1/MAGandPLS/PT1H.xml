      </TemporalDescription>
      <ObservedRegion>Heliosphere.Outer</ObservedRegion>
      <ObservedRegion>Heliosphere.Heliosheath</ObservedRegion>
      <Parameter>
         <Name>Year</Name>
         <ParameterKey>Column1</ParameterKey>
         <Description>Four digit Year</Description>
         <Cadence>PT1H</Cadence>
         <FillValue>999.9999</FillValue>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Day</Name>
         <ParameterKey>Column2</ParameterKey>
         <Description>Decimal Day of Year</Description>
         <Cadence>PT1H</Cadence>
         <FillValue>999.9999</FillValue>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Hour</Name>
         <ParameterKey>Column3</ParameterKey>
         <Description>Hour</Description>
         <Cadence>PT1H</Cadence>
         <FillValue>999.9999</FillValue>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>V1 radial distance</Name>
         <ParameterKey>Column04</ParameterKey>
         <Description>Heliocentric radial distance of Voyager 1 in AU</Description>
         <Cadence>PT1H</Cadence>
         <Units>AU</Units>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>V1 latitude</Name>
         <ParameterKey>Column05</ParameterKey>
         <Description>Heliographic Inertial elevation angle of Voyager 1 in deg</Description>
         <Cadence>PT1H</Cadence>
         <Units>deg</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Spherical</CoordinateRepresentation>
            <CoordinateSystemName>HGI</CoordinateSystemName>
         </CoordinateSystem>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>V1 longitude</Name>
         <ParameterKey>Column06</ParameterKey>
         <Description>Heliographic Inertial azimuthal angle of Voyager 1 in deg</Description>
         <Cadence>PT1H</Cadence>
         <Units>deg</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Spherical</CoordinateRepresentation>
            <CoordinateSystemName>HGI</CoordinateSystemName>
         </CoordinateSystem>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Average Field Magnitude</Name>
         <ParameterKey>Column07</ParameterKey>
         <Description>Hourly average of fine time scale magnetic field magnitudes</Description>
         <Cadence>PT1H</Cadence>
         <Units>nT</Units>
         <FillValue>999.999</FillValue>
         <Field>
            <Qualifier>Magnitude</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Magnitude of Average Field Vector</Name>
         <ParameterKey>Column08</ParameterKey>
         <Description>Magnitude of vector formed from hourly averages of fine time scale magnetic field components</Description>
         <Cadence>PT1H</Cadence>
         <Units>nT</Units>
         <FillValue>999.999</FillValue>
         <Field>
            <Qualifier>Magnitude</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Magnetic Field Vector</Name>
         <Description>Hourly averages of RTN components of fine time scale magnetic field vectors</Description>
         <Cadence>PT1H</Cadence>
         <Units>nT</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
            <CoordinateSystemName>RTN</CoordinateSystemName>
         </CoordinateSystem>
         <Structure>
            <Size>3</Size>
            <Description>R,T,N components</Description>
            <Element>
               <Name>R component</Name>
               <Qualifier>Component.I</Qualifier>
               <Index>1</Index>
               <ParameterKey>Column09</ParameterKey>
            </Element>
            <Element>
               <Name>T component</Name>
               <Qualifier>Component.J</Qualifier>
               <Index>2</Index>
               <ParameterKey>Column10</ParameterKey>
            </Element>
            <Element>
               <Name>N component</Name>
               <Qualifier>Component.K</Qualifier>
               <Index>3</Index>
               <ParameterKey>Column11</ParameterKey>
            </Element>
         </Structure>
         <FillValue>999.999</FillValue>
         <Field>
            <Qualifier>Vector</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
      <Parameter>
         <Name>Flow speed</Name>
         <ParameterKey>Column12</ParameterKey>
         <Description>Hourly averaged solar wind plasma flow speed</Description>
         <Cadence>PT1H</Cadence>
         <Units>km/s</Units>
         <FillValue>9999.9</FillValue>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <Qualifier>Average</Qualifier>
            <ParticleQuantity>FlowSpeed</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Flow elevation angle</Name>
         <ParameterKey>Column13</ParameterKey>
         <Description>Hourly averaged solar wind plasma flow elevation angle</Description>
         <Cadence>PT1H</Cadence>
         <Units>deg</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Spherical</CoordinateRepresentation>
            <CoordinateSystemName>RTN</CoordinateSystemName>
         </CoordinateSystem>
         <FillValue>9999.9</FillValue>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <Qualifier>DirectionAngle.ElevationAngle</Qualifier>
            <ParticleQuantity>FlowVelocity</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Flow azimuth angle</Name>
         <ParameterKey>Column14</ParameterKey>
         <Description>Hourly averaged solar wind plasma flow azimuthal angle</Description>
         <Cadence>PT1H</Cadence>
         <CoordinateSystem>
            <CoordinateRepresentation>Spherical</CoordinateRepresentation>
            <CoordinateSystemName>RTN</CoordinateSystemName>
         </CoordinateSystem>
         <FillValue>9999.9</FillValue>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <Qualifier>DirectionAngle.AzimuthAngle</Qualifier>
            <ParticleQuantity>FlowVelocity</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Density</Name>
         <ParameterKey>Column15</ParameterKey>
         <Description>Hourly averaged solar wind plasma density</Description>
         <Cadence>PT1H</Cadence>
         <Units>n/cc</Units>
         <FillValue>99.99999</FillValue>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <Qualifier>Average</Qualifier>
            <ParticleQuantity>NumberDensity</ParticleQuantity>
         </Particle>
      </Parameter>
      <Parameter>
         <Name>Temperature</Name>
         <ParameterKey>Column16</ParameterKey>
         <Description>Hourly averaged solar wind plasma temperature</Description>
         <Cadence>PT1H</Cadence>
         <Units>degK</Units>
         <FillValue>9999999.</FillValue>
         <Particle>
            <ParticleType>Proton</ParticleType>
            <Qualifier>Average</Qualifier>
            <ParticleQuantity>Temperature</ParticleQuantity>
         </Particle>
      </Parameter>
