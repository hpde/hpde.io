      <MeasurementType>ThermalPlasma</MeasurementType>
      <TemporalDescription>
         <TimeSpan>
            <StartDate>1994-11-15T12:53:43Z</StartDate>
            <RelativeStopDate>-P02D</RelativeStopDate>
         </TimeSpan>
         <Cadence>PT96S</Cadence>
      </TemporalDescription>
