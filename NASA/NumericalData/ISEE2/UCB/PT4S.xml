    <Parameter>
      <Name>Electron fluxes</Name>
      <Description>Electron fluxes at 5 energies: 1.4 - 1.6, 4.5 - 6.0, 19 - 280, 84 - 280, &gt; 200 keV</Description>
      <Cadence>PT4S</Cadence>
      <Units>1/cm**2-sec-ster-keV</Units>
      <Structure>
        <Size>5</Size>
        <Description>fluxes for each of 5 energy channels</Description>
