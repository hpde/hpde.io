        <Element>
          <Name>Phi</Name>
          <Qualifier>Component.K</Qualifier>
          <Index>3</Index>
          <ParameterKey>Phi</ParameterKey>
          <Units>degrees</Units>
        </Element>
      </Structure>
      <Support>
        <SupportQuantity>Positional</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Rubidium Vapor Magnetometer Magnitude</Name>
      <ParameterKey>Magnetic Field Magnitude</ParameterKey>
