      <AccessInformation>
         <RepositoryID>spase://GMU/Repository/SpaceWeatherLab</RepositoryID>
         <AccessURL>
            <URL>http://spaceweather.gmu.edu/seeds/secchi.php</URL>
            <Style>WebService</Style>
         </AccessURL>
         <Format>HTML</Format>
      </AccessInformation>
      <InstrumentID>spase://SMWG/Instrument/STEREO-A/SECCHI/Cor2</InstrumentID>
      <InstrumentID>spase://SMWG/Instrument/STEREO-B/SECCHI/Cor2</InstrumentID>
      <PhenomenonType>CoronalMassEjection</PhenomenonType>
      <TimeSpan>
         <StartDate>2007-03-05T00:00:00</StartDate>
         <RelativeStopDate>-P3D</RelativeStopDate>
      </TimeSpan>
      <Parameter>
         <Name>Event Number</Name>
         <Description>Identifier of a CME</Description>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Start time</Name>
         <Description>Start date and time of the detected CME</Description>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Event Movie</Name>
         <Description>A hyperlink pointing to the corresponding event movie; The text indicates Number of Frames containing the event</Description>
         <Support>
            <SupportQuantity>WebService</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Central PA</Name>
         <Description>Central Position Angle of CME</Description>
         <Units>degrees</Units>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Angular Width</Name>
         <Description>Angular Width of the CME</Description>
         <Units>degrees</Units>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Linear Fit</Name>
         <Description>CME Speed obtained from Linear fit to the Height-time with the halfmax-lead values.</Description>
         <Units>km/s</Units>
         <Support>
            <SupportQuantity>Velocity</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>2nd Order Fit</Name>
         <Description>A CME speed obtained from the 2nd order polynomial fit to the Height-time with the halfmax lead values</Description>
         <Units>m/s^2</Units>
         <Support>
            <SupportQuantity>Velocity</SupportQuantity>
         </Support>
      </Parameter>
   </Catalog>
