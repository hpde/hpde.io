<?xml version="1.0" encoding="UTF-8"?>
<Spase xmlns="http://www.spase-group.org/data/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.spase-group.org/data/schema  http://www.spase-group.org/data/schema/spase-2_6_1.xsd">
   <Version>2.6.1</Version>
   <Catalog>
      <ResourceID>spase://NASA/Catalog/TIMED/SEE/Flare_catalog</ResourceID>
      <ResourceHeader>
         <ResourceName>Catalog of Flare Events observed by the TIMED-SEE experiment.</ResourceName>
         <ReleaseDate>2024-07-22T10:09:02</ReleaseDate>
         <Description>This page is a catalog of flare events observed by the TIMED Solar EUV Experiment (SEE). Each row contains NOAA SEC flare information from the daily edited event reports for periods when TIMED-SEE was observing the sun anywhere between the start and stop times of these events. Events have been filtered to exclude periods when no appreciable increases were detected by SEE. </Description>
         <Contact>
            <PersonID>spase://SMWG/Person/Thomas.N.Woods</PersonID>
            <Role>PrincipalInvestigator</Role>
         </Contact>
         <InformationURL>
            <URL>https://lasp.colorado.edu/see/overview/</URL>
            <Description>Catalog overview </Description>
         </InformationURL>
      </ResourceHeader>
      <AccessInformation>
         <RepositoryID>spase://SMWG/Repository/TIMED/SEE_POC</RepositoryID>
         <AccessURL>
            <URL>https://lasp.colorado.edu/see/data/see-flare-catalog/</URL>
         </AccessURL>
         <Format>Text.ASCII</Format>
      </AccessInformation>
      <InstrumentID>spase://SMWG/Instrument/TIMED/SEE</InstrumentID>
      <PhenomenonType>SolarFlare</PhenomenonType>
      <TimeSpan>
         <StartDate>2002-03-09T00:00:00</StartDate>
         <StopDate>2007-06-03T23:59:59</StopDate>
      </TimeSpan>
      <Parameter>
         <Name>Year/Doy mo-dd</Name>
         <Description>Date of the event</Description>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>start hhmm</Name>
         <Description>Start time of the event</Description>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Peak</Name>
         <Description>Time of event peak</Description>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Stop </Name>
         <Description>Time at the end of the event </Description>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Class</Name>
         <Description>Class of event</Description>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Solar
Longitude (deg)</Name>
         <Description>Heliographic latitude of x-ray flare defined by SXI or H-alpha when no SXI location is reported.</Description>
         <Units>degrees</Units>
         <Support>
            <SupportQuantity>Orientation</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Solar Latitude (deg)</Name>
         <Description>Heliographic longitude of x-ray flare defined by SXI or H-alpha when no SXI location is reported</Description>
         <Units>degrees</Units>
         <Support>
            <SupportQuantity>Orientation</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Region</Name>
         <Description>Region of event</Description>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>Event</Name>
         <Description>NOAA Event number.</Description>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>SEE-XPS Index</Name>
         <Description>SEE-XPS Index - TIMED-SEE XPS (0.1-7 nm) Flare Index (1=no flare, &gt;1=flare).</Description>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>SEE-EGS Index</Name>
         <Description>TIMED-SEE EGS Flare Index (1=no flare, &gt;1=flare).</Description>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>SEE obs (seconds after peak)</Name>
         <Description>Center of the TIMED-SEE observation time relative to the flare peak expressed in seconds. SEE observations are approximately 180 seconds. Positive numbers indicate seconds after the peak, negative numbers indicate seconds prior to the peak.</Description>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
   </Catalog>
</Spase>
