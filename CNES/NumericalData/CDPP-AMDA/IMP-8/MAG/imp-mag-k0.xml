    <TemporalDescription>
      <TimeSpan>
        <StartDate>1973-10-29T23:59:53Z</StartDate>
        <StopDate>2000-06-10T23:59:38Z</StopDate>
        <Note>Instrument stopped working on 2000-06-10.</Note>
      </TimeSpan>
