    <Parameter>
      <Name>e_dusk</Name>
      <ParameterKey>c2_edusk</ParameterKey>
      <Description> Duskward Electric Field, In spin plane perp to x-gse at spin time resolution (4s)
            </Description>
      <UCD>phys.elecField</UCD>
      <Cadence>PT4S</Cadence>
      <Units>mV/m</Units>
      <UnitsConversion>1.0e-3&gt;V m^-1</UnitsConversion>
      <RenderingHints>
        <DisplayType>TimeSeries</DisplayType>
        <AxisLabel>E_dusk</AxisLabel>
        <ScaleMin>-20</ScaleMin>
        <ScaleMax>20</ScaleMax>
      </RenderingHints>
      <ValidMin>-700</ValidMin>
      <ValidMax>700</ValidMax>
      <FillValue>-1.0000e+31</FillValue>
      <Field>
        <Qualifier>Scalar</Qualifier>
        <FieldQuantity>Electric</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>e_spectral_density_F1</Name>
      <ParameterKey>c2_efw_f1</ParameterKey>
      <Description>E-field spectral density [0.3-10] Hz, from EFW waveform at spin time resolution (4s)
            </Description>
      <UCD>phys.density</UCD>
      <Cadence>PT4S</Cadence>
      <Units>(V/m)^2/Hz</Units>
      <RenderingHints>
        <DisplayType>TimeSeries</DisplayType>
        <AxisLabel>E_pow_f1</AxisLabel>
        <ScaleMin>1.0000e-12</ScaleMin>
        <ScaleMax>1.0000e-4</ScaleMax>
      </RenderingHints>
      <ValidMin>1.0000e-13</ValidMin>
      <ValidMax>0.0010</ValidMax>
      <FillValue>-1.0000e+31</FillValue>
      <Field>
        <Qualifier>Spectral</Qualifier>
        <FieldQuantity>Electric</FieldQuantity>
        <FrequencyRange>
          <Low>1</Low>
          <High>10</High>
          <Units>Hz</Units>
        </FrequencyRange>
      </Field>
    </Parameter>
    <Parameter>
      <Name>e_spectral_density_F2</Name>
      <ParameterKey>c2_efw_f2</ParameterKey>
      <Description>  E-field spectral density [10-180] Hz, from EFW waveform at spin time resolution (4s)
            </Description>
      <UCD>phys.density</UCD>
      <Cadence>PT4S</Cadence>
