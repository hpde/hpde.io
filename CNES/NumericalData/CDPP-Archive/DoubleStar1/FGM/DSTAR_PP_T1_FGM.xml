    <Parameter>
      <Name>Double Star 1 FGM DC Magnetic Field Vector Prime Parameter</Name>
      <Description>Double Star 1 FGM magnetic field vectors at spin resolution in GSE Coordinates in units of nT.</Description>
      <Units>nT</Units>
      <CoordinateSystem>
        <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
        <CoordinateSystemName>GSE</CoordinateSystemName>
      </CoordinateSystem>
      <Field>
        <Qualifier>Vector</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Double Star 1 FGM Magnetic Variance Summed Prime Parameter</Name>
      <Description>Double Star 1 FGM normalised magnetic variance including summed component variances at spin resolution.</Description>
      <Field>
        <Qualifier>Variance</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Double Star 1 FGM Magnetic Variance of the Magnitude Prime Parameter</Name>
      <Description>Double Star 1 FGM magnetic variance of the magnitude at spin resolution.</Description>
      <Field>
        <Qualifier>Variance</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Double Star 1 FGM Status Prime Parameter</Name>
      <Description>Double Star 1 FGM status at spin resolution.</Description>
      <Support>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
