        <Name>REGARDS</Name>
        <URL>https://regards.cnes.fr/user/cdpp/modules/1777?sp=DatasetName;q=DatasetName:DSTAR_SP_T2_FGM</URL>
        <Description>Access via CNES Data Archive for CDPP</Description>
      </AccessURL>
      <Format>CDF</Format>
    </AccessInformation>
    <ProcessingLevel>Calibrated</ProcessingLevel>
    <InstrumentID>spase://CNES/Instrument/CDPP-Archive/DoubleStar2/FGM</InstrumentID>
