    <Parameter>
      <Name>Double Star 2 FGM DC Magnetic Field Vector Summary Parameter</Name>
      <Description>Double Star 2 FGM magnetic field vectors at 60 s resolution in GSE Coordinates</Description>
      <Units>nT</Units>
      <CoordinateSystem>
        <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
        <CoordinateSystemName>GSE</CoordinateSystemName>
      </CoordinateSystem>
      <Field>
        <Qualifier>Vector</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Double Star 2 FGM Magnetic Variance Summed Summary Parameter</Name>
      <Description>Double Star 2 FGM normalised magnetic variance including summed component variances at 60 s resolution</Description>
      <Field>
        <Qualifier>Variance</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Double Star 2 FGM Magnetic Variance of the Magnitude Summary Parameter</Name>
      <Description>Double Star 2 FGM magnetic variance of the magnitude at 60 s resolution.</Description>
      <Field>
        <Qualifier>Variance</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
