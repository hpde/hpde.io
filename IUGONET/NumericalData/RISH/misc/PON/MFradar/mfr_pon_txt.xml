      <Parameter>
         <Name>height</Name>
         <Description>Height (above sea level)</Description>
         <Units>km</Units>
         <ValidMin>60</ValidMin>
         <ValidMax>100</ValidMax>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>year</Name>
         <Description>Year in the form YYYY where YYYY= Gregorian Year AD.</Description>
