      </SpatialCoverage>
      <Keyword>EARTH SCIENCE Atmosphere Altitude Geopotential Height Tropopause Hydrostatic Pressure Atmospheric Temperature Profiles Water Vapor Humidity Winds Wind Profiles</Keyword>
      <Parameter>
         <Name>date</Name>
         <Description>Date in the form YYYY/MM/DD where YYYY= Gregorian Year AD, MM = month of year (01-12), and DD = day of month (01-31).</Description>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>time</Name>
         <Description>Time in the form hh:mm:ss where hh = hour of day (0-23), mm = minute of hour (00-59) and ss = second of minute (00-59). Universal time is used for time data.</Description>
         <Support>
            <SupportQuantity>Temporal</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>height</Name>
         <Description>Height above seal level.</Description>
         <Units>km</Units>
         <Support>
            <SupportQuantity>Positional</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>pressure</Name>
         <Description>Pressure</Description>
         <Units>hPa</Units>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>temperature</Name>
         <Description>Temperature</Description>
         <Units>degree C</Units>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>relative humidity</Name>
         <Description>Relative humidity</Description>
         <Units>%</Units>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>zonal wind</Name>
         <Description>Zonal wind (eastward wind is positive.)</Description>
         <Units>m/s</Units>
         <CoordinateSystem>
            <CoordinateRepresentation>Spherical</CoordinateRepresentation>
            <CoordinateSystemName>GEO</CoordinateSystemName>
         </CoordinateSystem>
         <Support>
            <SupportQuantity>Other</SupportQuantity>
         </Support>
      </Parameter>
      <Parameter>
         <Name>meridional wind</Name>
         <Description>Meridional wind (northward wind is positive.)</Description>
         <Units>m/s</Units>
