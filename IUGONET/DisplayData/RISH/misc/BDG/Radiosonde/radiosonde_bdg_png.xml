            <CoordinateRepresentation>Spherical</CoordinateRepresentation>
            <CoordinateSystemName>WGS84</CoordinateSystemName>
         </CoordinateSystem>
         <NorthernmostLatitude>-6.894</NorthernmostLatitude>
         <SouthernmostLatitude>-6.894</SouthernmostLatitude>
         <EasternmostLongitude>107.588</EasternmostLongitude>
         <WesternmostLongitude>107.588</WesternmostLongitude>
         <MinimumElevation>0</MinimumElevation>
