      <Name>DWP Status Acf</Name>
      <ParameterKey>Status_Acf__CL_SP_DWP</ParameterKey>
      <Description>Status of the calculation of the autocorrelation function of electrons counts. Status[0]= 0 = Bad data. Status[0]= 1 = Use with caution. Status[0]= 2 = OK. Status[0]= 255 = Not Supplied.</Description>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <Qualifier>Scalar</Qualifier>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
