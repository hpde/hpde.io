        <Parameter>
            <Name>Time</Name>
            <ParameterKey>Epoch</ParameterKey>
            <Description>Time in miliseconds since CDF epoch</Description>
            <Units>ms</Units>
            <UnitsConversion>1e-3&gt;s</UnitsConversion>
