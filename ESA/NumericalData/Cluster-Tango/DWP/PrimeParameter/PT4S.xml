    <Parameter>
      <Name>DWP Status</Name>
      <ParameterKey>Status__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP status of the DWP instrument at spin resolution. Status[0]= 0 = Bad data. Status[0]= 1 = Use with caution. Status[0]= 2 = OK. Status[0]= 255 = Not Supplied.</Description>
      <Cadence>PT4S</Cadence>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <Qualifier>Scalar</Qualifier>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>DWP Status Acf</Name>
      <ParameterKey>Status_Acf__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP status of the calculation of the autocorrelation function of electrons counts at spin resolution. Status[0]= 0 = Bad data. Status[0]= 1 = Use with caution. Status[0]= 2 = OK. Status[0]= 255 = Not Supplied.</Description>
      <Cadence>PT4S</Cadence>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <Qualifier>Scalar</Qualifier>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>DWP Status Heea</Name>
      <ParameterKey>Status_Heea__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP status of the PEACE instrument High Energy Electron Analyser (HEEA) at spin resolution. Status[0]= 0 = Bad data. Status[0]= 1 = Use with caution. Status[0]= 2 = OK. Status[0]= 255 = Not Supplied.</Description>
      <Cadence>PT4S</Cadence>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <Qualifier>Scalar</Qualifier>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>DWP Status B</Name>
      <ParameterKey>Status_B__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP status of the FGM instrument. Status[0]= 0 = Bad data at spin resolution. Status[0]= 1 = Use with caution. Status[0]= 2 = OK. Status[0]= 255 = Not Supplied.</Description>
      <Cadence>PT4S</Cadence>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <Qualifier>Scalar</Qualifier>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Wave Experiment Consortium (WEC) status</Name>
      <ParameterKey>State_wec__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP Wave Experiment Consortium (WEC) on Cluster consists of 5 instruments at spin resolution. This flag gives the status of the entire concortium. Status[0]= 0 = Bad data. Status[0]= 1 = Use with caution. Status[0]= 2 = OK. Status[0]= 255 = Not Supplied.</Description>
      <Cadence>PT4S</Cadence>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <Qualifier>Scalar</Qualifier>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Wide Band Data (WBD) status</Name>
      <ParameterKey>Status_wbd__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP status of the Wide Band Data instrument. Status[0]= 0 = Bad data at spin resolution. Status[0]= 1 = Use with caution. Status[0]= 2 = OK. Status[0]= 255 = Not Supplied.</Description>
      <Cadence>PT4S</Cadence>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <Qualifier>Scalar</Qualifier>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Particle Correlator Significance</Name>
      <ParameterKey>Correl_signif__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP significance of the particle correlator determined from the autocorrelation functions of the time series of particle detector counts at spin resolution.</Description>
      <Cadence>PT4S</Cadence>
      <RenderingHints>
        <AxisLabel>Signif.</AxisLabel>
        <ScaleMin>0</ScaleMin>
        <ScaleMax>100</ScaleMax>
      </RenderingHints>
      <ValidMin>0</ValidMin>
      <ValidMax>100</ValidMax>
      <FillValue>-1.0e+31</FillValue>
      <Support>
        <Qualifier>Scalar</Qualifier>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Particle Correlator Energy Band</Name>
      <ParameterKey>Correl_P__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP energy band of peak correlation for the particle correlator at spin resolution.</Description>
      <Cadence>PT4S</Cadence>
      <Units>eV</Units>
      <RenderingHints>
        <AxisLabel>Energy</AxisLabel>
        <ScaleMin>0.1</ScaleMin>
        <ScaleMax>3200</ScaleMax>
      </RenderingHints>
      <ValidMin>0.6</ValidMin>
      <ValidMax>30022</ValidMax>
      <FillValue>-1.0e+31</FillValue>
      <Particle>
        <ParticleType>Electron</ParticleType>
        <Qualifier>Scalar</Qualifier>
        <ParticleQuantity>Energy</ParticleQuantity>
        <EnergyRange>
          <Low>0.6</Low>
          <High>26000</High>
          <Units>eV</Units>
        </EnergyRange>
      </Particle>
    </Parameter>
    <Parameter>
      <Name>Particle Correlator Frequency Band</Name>
      <ParameterKey>Correl_freq__C4_PP_DWP</ParameterKey>
      <Description>Cluster DWP frequency band of peak correlation for the particle correlator at spin resolution.</Description>
      <Cadence>PT4S</Cadence>
      <Units>kHz</Units>
      <RenderingHints>
        <AxisLabel>Frequency</AxisLabel>
        <ScaleMin>1.0</ScaleMin>
        <ScaleMax>50</ScaleMax>
      </RenderingHints>
      <ValidMin>1.3</ValidMin>
      <ValidMax>50</ValidMax>
      <FillValue>-1.0e+31</FillValue>
      <Wave>
        <WaveType>Electromagnetic</WaveType>
        <Qualifier>Scalar</Qualifier>
        <WaveQuantity>Frequency</WaveQuantity>
      </Wave>
    </Parameter>
