    <Parameter>
      <Name>Double Star 2 LEID Universal Time Summary Parameter</Name>
      <ParameterKey>Epoch__T2_SP_LEI</ParameterKey>
      <Description>Interval centered time tag rounded to nearest msecond. Universal Time in data cell: day, month, year hour, min, seconds. Ex: '01-Jan-1981 00:00:00.000'.</Description>
      <Cadence>PT60S</Cadence>
      <Units>ms</Units>
      <UnitsConversion>1.0e-3&gt;s</UnitsConversion>
      <RenderingHints>
        <AxisLabel>Epoch</AxisLabel>
</RenderingHints>
      <FillValue>-1.0e31</FillValue>
      <Support>
        <SupportQuantity>Temporal</SupportQuantity>
</Support>
</Parameter>
    <Parameter>
      <Name>Double Star 2 LEID Half Width of Measurement Interval Summary Parameter</Name>
      <ParameterKey>Half_interval__T2_SP_LEI</ParameterKey>
      <Description>Double Star 2 LEID half sample time interval in units of ms.</Description>
      <Cadence>PT60S</Cadence>
      <Units>ms</Units>
      <UnitsConversion>1.0e-3&gt;s</UnitsConversion>
      <FillValue>-1.0e31</FillValue>
      <Support>
        <SupportQuantity>Other</SupportQuantity>
</Support>
</Parameter>
    <Parameter>
      <Name>Double Star 2 LEID Status Summary Parameter</Name>
      <ParameterKey>Status__T2_SP_LEI</ParameterKey>
      <Description>Double Star 2 LEID status at 60 s resolution.</Description>
      <Cadence>PT60S</Cadence>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <SupportQuantity>Other</SupportQuantity>
</Support>
</Parameter>
    <Parameter>
      <Name>Double Star 2 LEID Low Energy Ion Density Summary Parameter</Name>
      <ParameterKey>N_ion__T2_SP_LEI</ParameterKey>
      <Description>Double Star 2 LEID low energy ion density at 60 s resolution in units of cm^-3.</Description>
      <Cadence>PT60S</Cadence>
      <Units>cm^-3</Units>
      <UnitsConversion>1.0e6&gt;m^-3</UnitsConversion>
      <RenderingHints>
        <AxisLabel>n (LEID)</AxisLabel>
        <ScaleMin>0.0100</ScaleMin>
        <ScaleMax>1000</ScaleMax>
</RenderingHints>
      <ValidMin>0.0100</ValidMin>
      <ValidMax>1000</ValidMax>
      <FillValue>-1.0e31</FillValue>
      <Particle>
        <ParticleType>Ion</ParticleType>
        <Qualifier>Scalar</Qualifier>
        <ParticleQuantity>NumberDensity</ParticleQuantity>
</Particle>
</Parameter>
