    <Parameter>
      <Name>Double Star 2 FGM Universal Time Summary Parameter</Name>
      <ParameterKey>Epoch__T2_SP_FGM</ParameterKey>
      <Description>Universal Time in data cell: day, month, year hour, min, seconds. Ex: '01-Jan-1981 00:00:00.000'. The time is an interval centered time tag rounded to nearest msecond.</Description>
      <Cadence>PT60S</Cadence>
      <Units>ms</Units>
      <UnitsConversion>1.0e-3&gt;s</UnitsConversion>
      <RenderingHints>
        <AxisLabel>Epoch</AxisLabel>
</RenderingHints>
      <FillValue>-1.0e31</FillValue>
      <Support>
        <SupportQuantity>Temporal</SupportQuantity>
</Support>
</Parameter>
    <Parameter>
      <Name>Double Star 2 FGM Half Interval Summary Parameter</Name>
      <ParameterKey>Half_interval__T2_SP_FGM</ParameterKey>
      <Description>Double Star 2 FGM half the mean separation between time tags, averaged over the file.</Description>
      <Cadence>PT60S</Cadence>
      <Units>ms</Units>
      <UnitsConversion>1.0e-3&gt;s</UnitsConversion>
      <RenderingHints>
        <AxisLabel>Half sample time interval</AxisLabel>
</RenderingHints>
      <FillValue>-1.0e31</FillValue>
      <Support>
        <SupportQuantity>Other</SupportQuantity>
</Support>
</Parameter>
    <Parameter>
      <Name>Double Star 2 FGM Status Summary Parameter</Name>
      <ParameterKey>Status__T2_SP_FGM</ParameterKey>
      <Description>Double Star 2 FGM status at 60 s resolution.</Description>
      <Cadence>PT60S</Cadence>
      <ValidMin>0</ValidMin>
      <ValidMax>255</ValidMax>
      <FillValue>255</FillValue>
      <Support>
        <SupportQuantity>Other</SupportQuantity>
</Support>
</Parameter>
